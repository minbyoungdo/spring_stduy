<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.ProductMapper">
	<select id="getlist" resultType="com.example.domain.Product_CategoryVO">
		<![CDATA[select * from tbl_product_category
		]]>
	</select>
	<select id="get" resultType="com.example.domain.Product_CategoryVO">
		select * from tbl_product_category
			where pcategory=#{category}
	</select>
	<select id="getcount" resultType="int">
		select count(*) from tbl_proudct_category
	</select>
</mapper>